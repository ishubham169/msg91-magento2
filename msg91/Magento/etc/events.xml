<?xml version="1.0"?>
<config xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:framework:Event/etc/events.xsd">
    <event name="checkout_submit_all_after">
        <observer name="tgm_voodoo_observer_new_order" instance="msg91\Magento\Observer\NewOrder"/>
    </event>
    <event name="sales_order_save_after">
        <observer name="tgm_voodoo_observer_order_cancelled" instance="msg91\Magento\Observer\OrderCancelled"/>
        <observer name="tgm_voodoo_observer_order_hold" instance="msg91\Magento\Observer\OrderHold"/>
        <observer name="tgm_voodoo_observer_order_un_hold" instance="msg91\Magento\Observer\OrderUnHold"/>
    </event>
    <event name="sales_order_invoice_pay">
        <observer name="tgm_voodoo_observer_invoice_pay" instance="msg91\Magento\Observer\InvoicePay"/>
    </event>
    <event name="customer_register_success">
        <observer name="tgm_voodoo_observer_register_success" instance="msg91\Magento\Observer\Registration"/>
    </event>
</config>